<div class="container">
    <div class="card" style="width: 18rem;">
        <img class="card-img-top" src="{{user.image}}" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">{{user.fullname}}</h5>
            <p class="card-text">{{user.college}}</p>
        </div>
    </div>
</div>